Main.Name = "Main"
Main.Parent = DoomspirebrickbattleGUI
Main.BackgroundColor3 = Color3.new(0.168627, 0.168627, 0.168627)
Main.Position = UDim2.new(0.0901835635, 0, 0.104368925, 0)
